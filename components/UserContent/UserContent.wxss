.user-content-page {
  margin-top: 20rpx;
  color: #666;
}
.user-content-page .user-title-wrapper {
  display: flex;
  border-bottom: 15rpx solid #dedede;
}
.user-content-page .user-title-wrapper .content {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.user-content-page .user-title-wrapper .content :first-child {
  color: var(--themeColor);
  font-size: 32rpx;
}
.user-content-page .user-list-title {
  padding: 16rpx 16rpx;
  border-bottom: 3rpx solid #dedede;
}
.user-content-page .user-list-content {
  padding: 16rpx 0;
  display: flex;
  border-bottom: 15rpx solid #dedede;
}
.user-content-page .user-list-content .content {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.user-content-page .user-list-content .content :first-child {
  font-size: 36rpx;
}
.user-content-page .user-list-content .content :first-child :first-child {
  color: var(--themeColor);
}
.user-content-page .address-management {
  padding: 16rpx 16rpx;
  border-bottom: 15rpx solid #dedede;
}
.user-content-page .contact-cs {
  display: flex;
  padding: 16rpx 16rpx;
  justify-content: space-between;
  border-bottom: 3rpx solid #dedede;
}
.user-content-page .to-feedback {
  padding: 16rpx 16rpx;
  border-bottom: 3rpx solid #dedede;
}
.user-content-page .about-us {
  padding: 16rpx 16rpx;
  border-bottom: 15rpx solid #dedede;
}
.user-content-page .share-to-other {
  padding: 16rpx 16rpx;
}
